{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "allowImportExportEverywhere": true
  },
  "env": {
      "browser": true
  },
  "rules": {
      // "indent": [2],
      "semi" : [1,"never"],
      "linebreak-style": 0,
      "no-extra-semi":"warn"
      // "no-restricted-syntax":"warn",
      // "prefer-destructuring": "warn",
      // "react/jsx-indent": 0,
      // "no-underscore-dangle": 0,
      // "comma-dangle":0,
      // "function-paren-newline": 0,
      // "import/no-extraneous-dependencies": 0
      // "import/newline-after-import": 0
  }
}